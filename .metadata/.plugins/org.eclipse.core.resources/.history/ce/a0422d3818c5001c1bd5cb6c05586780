package Issue13;

public class HeroApp {

	public static void main(String[] args) {
		// TODO 自動生成されたメソッド・スタブ
		Hero h = new Hero("TAKAHIRO",234);
		h.setName("Takahiro");
		Wand wand = new Wand();
		wand.setName("美しすぎるEXILEの杖");
		wand.setPower(25.0);
		Wizard w = new Wizard("Makidai",-23,21,wand);
		w.heal(h);
	}

}
